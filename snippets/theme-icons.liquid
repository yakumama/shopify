{% comment %}
    Render icon for Charge Theme

    Accepts:
    - name:      {String} Icon name
    - css_class: {String} Additional CSS classes for svg

    Usage:
    {% render 'theme-icons', name: 'email', css_class: 'my-class' %}
{% endcomment %}

{%- assign css_class = css_class | default: '' -%}

