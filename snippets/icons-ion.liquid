{% comment %}
    Render Ion Icon

    Accepts:
    - name:      {String} Icon name
    - css_class: {String} Additional CSS classes for svg

    Usage:
    {% render 'icons-ion', name: 'email', css_class: 'w-6 h-6' %}
{% endcomment %}

{%- assign css_class = css_class | default: '' -%}

{%- capture svg -%}
    {%- case name -%}

